<%- include ("./partials/header") %>
<div class = "container">
<div class="jumbotron jumbotron-fluid" id="SignUp">
  <div class="container">
    <h1 class="display-4">Sign Up</h1>
    <p class="lead">Sign Up to create/edit campgrounds and comments!</p>
  </div>
</div>


<form action="/register" method="post">
	<input type="text" name="username" placeholder="username" class="d-block my-2">
	<input type="password" name="password" placeholder="password" class="d-block my-2">
	<button type = submit class ="btn btn-primary">Sign up!</button>
</form>
</div>
<%- include ("./partials/footer") %>